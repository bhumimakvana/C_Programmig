#include<stdio.h>

/*
Q-1.Write a Program to find the cube of a given number using UDF.
	For example,
	Input:
	Enter any number: 5
	Output:
	Cube is: 125
*/


main(){
	int n;
	clrscr();
	printf("Enter the nub:")
	scanf("%d",&n);
	printf("Factorial of %d is %|d",n,factorial(n));
	getch();
}
long int factorial(int n);
{
	if
	(n<=1)
}
{
	return(01);
}
else
{
n=n*factorial(n-1);
return(n);
}

